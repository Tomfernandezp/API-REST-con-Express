#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Tomi/Desktop/Programacion/Midulive/Curso Node JS/Clase 5 - Base de datos/node_modules/mime/node_modules:/mnt/c/Users/Tomi/Desktop/Programacion/Midulive/Curso Node JS/Clase 5 - Base de datos/node_modules:/mnt/c/Users/Tomi/Desktop/Programacion/Midulive/Curso Node JS/node_modules:/mnt/c/Users/Tomi/Desktop/Programacion/Midulive/node_modules:/mnt/c/Users/Tomi/Desktop/Programacion/node_modules:/mnt/c/Users/Tomi/Desktop/node_modules:/mnt/c/Users/Tomi/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/Tomi/Desktop/Programacion/Midulive/Curso Node JS/Clase 5 - Base de datos/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Tomi/Desktop/Programacion/Midulive/Curso Node JS/Clase 5 - Base de datos/node_modules/mime/node_modules:/mnt/c/Users/Tomi/Desktop/Programacion/Midulive/Curso Node JS/Clase 5 - Base de datos/node_modules:/mnt/c/Users/Tomi/Desktop/Programacion/Midulive/Curso Node JS/node_modules:/mnt/c/Users/Tomi/Desktop/Programacion/Midulive/node_modules:/mnt/c/Users/Tomi/Desktop/Programacion/node_modules:/mnt/c/Users/Tomi/Desktop/node_modules:/mnt/c/Users/Tomi/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/Tomi/Desktop/Programacion/Midulive/Curso Node JS/Clase 5 - Base de datos/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../mime/cli.js" "$@"
else
  exec node  "$basedir/../mime/cli.js" "$@"
fi
